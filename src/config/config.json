{
  "site": {
    "title": "有限会社 南西流通ライン",
    "base_url": "https://astroplate.netlify.app",
    "base_path": "/",
    "trailing_slash": false,
    "favicon": "/images/favicon.png",
    "logo": "/images/logo_hr_b.png",
    "logo_darkmode": "/images/logo_hr_w.png",
    "logo_width": "200",
    "logo_height": "25",
    "logo_text": "ロゴ：有限会社 南西流通ライン"
  },

  "settings": {
    "search": true,
    "sticky_header": true,
    "theme_switcher": true,
